{
  "expo": {
    "name": "Blog Posts",
    "slug": "react-native-self-posts",
    "privacy": "public",
    "sdkVersion": "45.0.0",
    "platforms": ["ios","android","web"],
    "version": "1.3.0",
    "orientation": "default",
    "icon": "./assets/rn-icon.png",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vjappcoder.self-blog-rnapp",
      "buildNumber": "1.3.0"
    },
    "android": {
      "package": "com.vjappcoder.self_blog_rnapp",
      "versionCode": 5,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
